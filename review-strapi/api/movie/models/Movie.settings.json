{
  "connection": "default",
  "collectionName": "movies",
  "info": {
    "name": "movie",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "premiere": {
      "type": "date"
    },
    "studio": {
      "model": "studio",
      "via": "movies"
    },
    "moviegenres": {
      "collection": "moviegenre",
      "via": "movies"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "reviews": {
      "collection": "review",
      "via": "movie"
    },
    "movieplatforms": {
      "collection": "movieplatform",
      "via": "movies"
    }
  }
}